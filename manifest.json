{
  "name": "WebBook",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Tools for Highlighting pages, making notes",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
  	"scripts": ["background.js"]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "tabs"
  ]
}
